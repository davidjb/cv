  "bugs": {
    "url": "https://github.com/davidjb/cv/issues"
  },
  "homepage": "https://github.com/davidjb/cv#readme",
  "dependencies": {
    "gh-pages": "^4.0.0",
    "handlebars": "^4.7.7",
    "helper-markdown": "^1.0.0",
    "resume-cli": "patch:resume-cli@3.0.4#./patches/fix-493.patch",
    "yamljs": "^0.3.0"
  },
  "resolutions": {
    "puppeteer": "^15.4.2"
  }
}
