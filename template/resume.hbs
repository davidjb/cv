<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, user-scalable=no, minimal-ui">

    {{#resume.basics}}
    {{#name}}
    <title>{{.}} - Resumé</title>
    {{/name}}
    {{#picture}}
    <link rel="shortcut icon" href="{{.}}" />
    {{/picture}}
    {{/resume.basics}}

    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/octicons/2.0.2/octicons.min.css">

    <style>
    {{{css}}}
    </style>
  </head>
  <body>
    <a class="sr-only" href="#main">Skip to main content</a>

    <main id="main">
      {{#resume.basics}}
      <header class="bg-light py-3 pt-md-5 pb-md-4  mb-3 mb-md-4" id="header">
        <div class="container">
          <div class="row align-items-center">
            {{#picture}}
            <div class="col-4 col-sm-2 offset-sm-1 text-center">
              <img class="img-fluid" src="{{.}}" alt="Picture of {{../name}}">
            </div>
            {{/picture}}
            <div class="col col-sm-9">
              {{#name}}<h1 class="mb-1">{{.}}</h1>{{/name}}
              {{#label}}<div class="h3 text-secondary">{{.}}</div>{{/label}}
            </div>
          </div>
        </div>
      </header>
      {{/resume.basics}}

      <div id="content" class="container">
        {{#resume.basics}}
        <section id="contact" class="row">
          <div class="col-sm-3">
            <h2>Contact</h2>
          </div>
          <div class="col-sm-9">
            <div class="row row-cols-1 row-cols-sm-2">
            {{#email}}
            <div class="col contact">
              <strong>Email</strong>
              <div class="email">
                <a href="mailto:{{.}}">{{.}}</a>
              </div>
            </div>
            {{/email}}
            {{#phone}}
            <div class="col contact">
              <strong>Phone</strong>
              <div class="phone">{{.}}</div>
            </div>
            {{/phone}}
            {{#website}}
            <div class="col contact">
              <strong>Website</strong>
              <div class="website">
                <a href="{{.}}">{{.}}</a>
              </div>
            </div>
            {{/website}}
            </div>
          </div>
        </section>
        <section id="about" class="row">
          <div class="col-sm-3">
            <h2>About</h2>
          </div>
          <div class="col-sm-9 lead about">
          {{#summary}}{{{markdown this}}}{{/summary}}
          </div>
        </section>
        {{/resume.basics}}

        <nav class="navbar navbar-expand-md navbar-light bg-light sticky-top my-5">
          {{#resume.basics}}
          <a class="navbar-brand d-md-none" href="#main">
            {{#picture}}
            <img class="mr-1" src="{{.}}" alt="Picture of {{../name}}" width="30" height="30">
            {{/picture}}
            {{name}}
          </a>
          {{/resume.basics}}
          <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarResume" aria-controls="navbarResume" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse justify-content-md-center" id="navbarResume">
            <ul class="navbar-nav">
              {{#if profiles.length}}
              <li class="nav-item">
                <a class="nav-link" href="#profiles">Profiles</a>
              </li>
              {{/if}}
              {{#if resume.work.length}}
              <li class="nav-item">
                <a class="nav-link" href="#employment">Employment</a>
              </li>
              {{/if}}
              {{#if resume.projects.length}}
              <li class="nav-item">
                <a class="nav-link" href="#projects">Projects</a>
              </li>
              {{/if}}
              {{#if resume.volunteer.length}}
              <li class="nav-item">
                <a class="nav-link" href="#volunteering">Volunteering</a>
              </li>
              {{/if}}
              {{#if resume.education.length}}
              <li class="nav-item">
                <a class="nav-link" href="#education">Education</a>
              </li>
              {{/if}}
              {{#if resume.awards.length}}
              <li class="nav-item">
                <a class="nav-link" href="#awards">Awards</a>
              </li>
              {{/if}}
              {{#if resume.publications.length}}
              <li class="nav-item">
                <a class="nav-link" href="#publications">Publications</a>
              </li>
              {{/if}}
              {{#if resume.skills.length}}
              <li class="nav-item">
                <a class="nav-link" href="#skills">Skills</a>
              </li>
              {{/if}}
              {{#if resume.languages.length}}
              <li class="nav-item">
                <a class="nav-link" href="#languages">Languages</a>
              </li>
              {{/if}}
              {{#if resume.interests.length}}
              <li class="nav-item">
                <a class="nav-link" href="#interests">Interests</a>
              </li>
              {{/if}}
              {{#if resume.references.length}}
              <li class="nav-item">
                <a class="nav-link" href="#references">References</a>
              </li>
              {{/if}}
            </ul>
          </div>
        </nav>

        {{#resume.basics}}
        {{#if profiles.length}}
        <section id="profiles" class="row">
          <div class="col-sm-3">
            <h2>Profiles</h2>
          </div>
          <div class="col-sm-9">
            <div class="row row-cols-1 row-cols-sm-2 row-cols-md-3">
              {{#profiles}}
              <div class="col profile">
                {{#network}}
                <strong class="network">
                  {{.}}
                </strong>
                {{/network}}
                {{#if username}}
                <div class="username">
                  {{#if url}}
                  <div class="url">
                    <a href="{{url}}">{{username}}</a>
                  </div>
                  {{else}}
                    {{username}}
                  {{/if}}
                </div>
                {{else}}
                  {{#if url}}
                  <div class="url">
                    <a href="{{url}}">{{url}}</a>
                  </div>
                  {{/if}}
                {{/if}}
              </div>
              {{/profiles}}
            </div>
          </div>
        </section>
        {{/if}}
        {{/resume.basics}}

        {{#if resume.work.length}}
        <section id="employment" class="row">
          <div class="col-sm-3">
            <h2>Employment</h2>
          </div>
          <div class="col-sm-9">
            <div class="row row-cols-1">
            {{#each resume.work}}
            <div class="col work">
              <h3 class="strike-through">
                <span>{{company}}</span>
                <span class="date">
                  {{startDate}} — {{#endDate}}{{.}}{{else}}Present{{/endDate}}
                </span>
              </h3>
              {{#website}}
              <div class="website">
                <a href="{{.}}">{{.}}</a>
              </div>
              {{/website}}
              {{#position}}
              <div class="position">
                {{.}}
              </div>
              {{/position}}
              {{#summary}}
              <div class="summary lead">{{{markdown this}}}</div>
              {{/summary}}
              {{#if highlights.length}}
              <div class="h4">Highlights</div>
              <ul class="list-fancy highlights">
                {{#highlights}}
                <li>{{{markdown this}}}</li>
                {{/highlights}}
              </ul>
              {{/if}}
            </div>
            {{/each}}
            </div>
          </div>
        </section>
        {{/if}}

        {{#if resume.projects.length}}
        <section id="projects" class="row">
          <div class="col-sm-3">
            <h2>Projects</h2>
          </div>
          <div class="col-sm-9">
            <div class="row row-cols-1">
            {{#each resume.projects}}
            <div class="col project">
              <h3 class="strike-through">
                <span>{{entity}}</span>
                <span class="date">
                  {{startDate}} — {{#endDate}}{{.}}{{else}}Present{{/endDate}}
                </span>
              </h3>
              {{#url}}
              <div class="url">
                <a href="{{.}}">{{.}}</a>
              </div>
              {{/url}}
              {{#name}}
              <div class="name">
                <span class="type">{{../type}}</span> – {{.}}
              </div>
              {{/name}}
              {{#description}}
              <div class="summary lead">
                {{{markdown this}}}
              </div>
              {{/description}}
              <div class="row row-cols-2">
                {{#if highlights.length}}
                <div class="col">
                  <div class="h4">Highlights</div>
                  <ul class="list-fancy highlights">
                    {{#highlights}}
                    <li>{{{markdown this}}}</li>
                    {{/highlights}}
                  </ul>
                </div>
                {{/if}}
                {{#if roles.length}}
                <div class="col">
                  <div class="h4">Roles</div>
                  <ul class="list-fancy roles">
                    {{#roles}}
                    <li>{{{markdown this}}}</li>
                    {{/roles}}
                  </ul>
                </div>
                {{/if}}
              </div>
            </div>
            {{/each}}
            </div>
          </div>
        </section>
        {{/if}}

        {{#if resume.volunteer.length}}
        <section id="volunteering" class="row">
          <div class="col-sm-3">
            <h2>Volunteering</h2>
          </div>
          <div class="col-sm-9">
            <div class="row row-cols-1">
            {{#each resume.volunteer}}
            <div class="col volunteer">
              <h3 class="strike-through">
                <span>{{organization}}</span>
                <span class="date">
                  {{startDate}} — {{#endDate}}{{.}}{{else}}Present{{/endDate}}
                </span>
              </h3>
              {{#website}}
              <div class="website">
                <a href="{{.}}">{{.}}</a>
              </div>
              {{/website}}
              {{#position}}
              <div class="position">
                {{.}}
              </div>
              {{/position}}
              {{#summary}}
              <div class="summary lead">
                {{{markdown this}}}
              </div>
              {{/summary}}
              {{#if highlights.length}}
              <div class="h4">Highlights</div>
              <ul class="list-fancy highlights">
                {{#highlights}}
                <li>{{{markdown this}}}</li>
                {{/highlights}}
              </ul>
              {{/if}}
            </div>
            {{/each}}
            </div>
          </div>
        </section>
        {{/if}}

        {{#if resume.education.length}}
        <section id="education" class="row">
          <div class="col-sm-3">
            <h2>Education</h2>
          </div>
          <div class="col-sm-9">
            <div class="row row-cols-1">
            {{#each resume.education}}
            <div class="col education">
              <h3 class="strike-through">
                <span>{{institution}}</span>
                <span class="date">
                  {{startDate}} — {{#endDate}}{{.}}{{else}}Present{{/endDate}}
                </span>
              </h3>
              {{#area}}
              <div class="area">
                {{.}}
              </div>
              {{/area}}
              {{#studyType}}
              <div class="studyType">
                {{.}}
              </div>
              {{/studyType}}
              {{#if courses.length}}
              <h3>Courses</h3>
              <ul class="list-fancy courses">
                {{#courses}}
                <li>{{.}}</li>
                {{/courses}}
              </ul>
              {{/if}}
            </div>
            {{/each}}
            </div>
          </div>
        </section>
        {{/if}}

        {{#if resume.awards.length}}
        <section id="awards" class="row">
          <div class="col-sm-3">
            <h2>Awards</h2>
          </div>
          <div class="col-sm-9">
            <div class="row row-cols-1">
            {{#each resume.awards}}
            <div class="col award">
              <h3 class="strike-through">
                <span>{{title}}</span>
              </h3>
              {{#date}}
              <div class="date">
                <em>Awarded</em>
                {{.}}
              </div>
              {{/date}}
              {{#awarder}}
              <div class="awarder">
                <em>by</em>
                <strong>{{.}}</strong>
              </div>
              {{/awarder}}
              {{#summary}}
              <div class="summary lead">
                {{{markdown this}}}
              </div>
              {{/summary}}
            </div>
            {{/each}}
            </div>
          </div>
        </section>
        {{/if}}

        {{#if resume.publications.length}}
        <section id="publications" class="row">
          <div class="col-sm-3">
            <h2>Publications</h2>
          </div>
          <div class="col-sm-9">
            <div class="row row-cols-1">
            {{#each resume.publications}}
            <div class="col publication">
              <h3 class="strike-through">
                <span>{{name}}</span>
                <span class="date">
                  {{releaseDate}}
                </span>
              </h3>
              {{#website}}
              <div class="website">
                <a href="{{.}}"></a>
              </div>
              {{/website}}
              {{#publisher}}
              <div class="publisher">
                <em>Published by</em>
                <strong>{{.}}</strong>
              </div>
              {{/publisher}}
              {{#summary}}
              <div class="summary lead">
                {{{markdown this}}}
              </div>
              {{/summary}}
            </div>
            {{/each}}
            </div>
          </div>
        </section>
        {{/if}}

        {{#if resume.skills.length}}
        <section id="skills" class="row">
          <div class="col-sm-3">
            <h2>Skills</h2>
          </div>
          <div class="col-sm-9">
            <div class="row row-cols-1 row-cols-sm-2">
            {{#each resume.skills}}
            <div class="col skill">
              {{#name}}
              <div class="name">
                <h3>{{.}}</h3>
              </div>
              {{/name}}
              {{#if keywords.length}}
              <ul class="list-fancy keywords">
                {{#keywords}}
                <li>{{{markdown this}}}</li>
                {{/keywords}}
              </ul>
              {{/if}}
            </div>
            {{/each}}
            </div>
          </div>
        </section>
        {{/if}}

        {{#if resume.languages.length}}
        <section id="languages" class="row">
          <div class="col-sm-3">
            <h2>Languages</h2>
          </div>
          <div class="col-sm-9">
            <div class="row row-cols-1 row-cols-sm-2">
            {{#each resume.languages}}
            <div class="col">
              {{#language}}
              <div class="language">
                <strong>{{.}}</strong>
              </div>
              {{/language}}
              {{#fluency}}
              <div class="fluency">
                {{.}}
              </div>
              {{/fluency}}
            </div>
            {{/each}}
            </div>
          </div>
        </section>
        {{/if}}

        {{#if resume.interests.length}}
        <section id="interests" class="row">
          <div class="col-sm-3">
            <h2>Interests</h2>
          </div>
          <div class="col-sm-9">
            <div class="row row-cols-1 row-cols-sm-2">
            {{#each resume.interests}}
            <div class="col">
              {{#name}}
              <div class="name">
                <h3>{{.}}</h3>
              </div>
              {{/name}}
              {{#if keywords.length}}
              <ul class="list-fancy keywords">
                {{#keywords}}
                <li>{{.}}</li>
                {{/keywords}}
              </ul>
              {{/if}}
            </div>
            {{/each}}
            </div>
          </div>
        </section>
        {{/if}}

        {{#if resume.references.length}}
        <section id="references" class="row">
          <div class="col-sm-3">
            <h2>References</h2>
          </div>
          <div class="col-sm-9">
            <div class="row row-cols-1">
            {{#each resume.references}}
            <div class="col">
              {{#if reference}}
              <blockquote class="blockquote">
                <div class="reference">
                {{{markdown reference}}}
                </div>
                {{#name}}
                <footer class="blockquote-footer">
                  {{.}}
                </footer>
                {{/name}}
              </blockquote>
              {{/if}}
            </div>
            {{/each}}
            </div>
          </div>
        </section>
        {{/if}}
      </div>
    </main>

    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js" integrity="sha384-OgVRvuATP1z7JjHLkuOU7Xw704+h835Lr+6QL9UvYjZE3Ipu6Tp75j7Bh/kR0JKI" crossorigin="anonymous"></script>
    <script>
      $('.navbar-nav > li > a').on('click', function() {
        $('.navbar-collapse').collapse('hide')
      })
    </script>
  </body>
</html>
