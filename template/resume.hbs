<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, user-scalable=no, minimal-ui">

    {{#resume.basics}}
    {{#name}}
    <title>{{.}} - Resumé</title>
    {{/name}}
    {{#picture}}
    <link rel="shortcut icon" href="{{.}}" />
    {{/picture}}
    {{/resume.basics}}

    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/octicons/2.0.2/octicons.min.css">

    <style>
    {{{css}}}
    </style>
  </head>
  <body>
    <a class="sr-only" href="#main">Skip to main content</a>

    <main id="main">
      {{#resume.basics}}
      <header class="bg-light py-3 pt-md-5 pb-md-4  mb-3 mb-md-4" id="header">
        <div class="container">
          <div class="row align-items-center">
            {{#picture}}
            <div class="col-4 col-sm-2 offset-sm-1 text-center">
              <img class="img-fluid" src="{{.}}" alt="Picture of {{../name}}">
            </div>
            {{/picture}}
            <div class="col col-sm-9">
              {{#name}}<h1 class="mb-1">{{.}}</h1>{{/name}}
              {{#label}}<div class="h3 text-secondary">{{.}}</div>{{/label}}
            </div>
          </div>
        </div>
      </header>
      {{/resume.basics}}

      <div id="content" class="container">
        {{#resume.basics}}
        <section id="contact" class="row">
          <div class="col-sm-3">
            <h2>Contact</h2>
          </div>
          <div class="col-sm-9">
            <div class="row">
            {{#email}}
            <div class="col-sm-6 contact">
              <strong>Email</strong>
              <div class="email">
                <a href="mailto:{{.}}">{{.}}</a>
              </div>
            </div>
            {{/email}}
            {{#phone}}
            <div class="col-sm-6 contact">
              <strong>Phone</strong>
              <div class="phone">{{.}}</div>
            </div>
            {{/phone}}
            {{#website}}
            <div class="col-sm-6 contact">
              <strong>Website</strong>
              <div class="website">
                <a href="{{.}}">{{.}}</a>
              </div>
            </div>
            {{/website}}
            </div>
          </div>
        </section>
        <section id="about" class="row">
          <div class="col-sm-3">
            <h2>About</h2>
          </div>
          <div class="col-sm-9 lead about">
          {{#summary}}{{{markdown this}}}{{/summary}}
          </div>
        </section>
        {{#if profiles.length}}
        <section id="profiles" class="row">
          <div class="col-sm-3">
            <h2>Profiles</h2>
          </div>
          <div class="col-sm-9">
            <div class="row">
              {{#profiles}}
              <div class="col-sm-6 col-md-4 profile">
                {{#network}}
                <strong class="network">
                  {{.}}
                </strong>
                {{/network}}
                {{#if username}}
                <div class="username">
                  {{#if url}}
                  <div class="url">
                    <a href="{{url}}">{{username}}</a>
                  </div>
                  {{else}}
                    {{username}}
                  {{/if}}
                </div>
                {{else}}
                  {{#if url}}
                  <div class="url">
                    <a href="{{url}}">{{url}}</a>
                  </div>
                  {{/if}}
                {{/if}}
              </div>
              {{/profiles}}
            </div>
          </div>
        </section>
        {{/if}}
        {{/resume.basics}}

        {{#if resume.work.length}}
        <section id="work" class="row">
          <div class="col-sm-3">
            <h2>Employment</h2>
          </div>
          <div class="col-sm-9">
            <div class="row">
            {{#each resume.work}}
            <div class="col-12 work">
              <h3 class="strike-through">
                <span>{{company}}</span>
                <span class="date">
                  {{startDate}} — {{#endDate}}{{.}}{{else}}Present{{/endDate}}
                </span>
              </h3>
              {{#website}}
              <div class="website">
                <a href="{{.}}">{{.}}</a>
              </div>
              {{/website}}
              {{#position}}
              <div class="position">
                {{.}}
              </div>
              {{/position}}
              {{#summary}}
              <div class="summary lead">{{{markdown this}}}</div>
              {{/summary}}
              {{#if highlights.length}}
              <div class="h4">Highlights</div>
              <ul class="highlights">
                {{#highlights}}
                <li class="bullet">{{{markdown this}}}</li>
                {{/highlights}}
              </ul>
              {{/if}}
            </div>
            {{/each}}
            </div>
          </div>
        </section>
        {{/if}}

        {{#if resume.projects.length}}
        <section id="projects" class="row">
          <div class="col-sm-3">
            <h2>Projects</h2>
          </div>
          <div class="col-sm-9">
            <div class="row">
            {{#each resume.projects}}
            <div class="col-12 project">
              <h3 class="strike-through">
                <span>{{entity}}</span>
                <span class="date">
                  {{startDate}} — {{#endDate}}{{.}}{{else}}Present{{/endDate}}
                </span>
              </h3>
              {{#url}}
              <div class="url">
                <a href="{{.}}">{{.}}</a>
              </div>
              {{/url}}
              {{#name}}
              <div class="name">
                <span class="type">{{../type}}</span> – {{.}}
              </div>
              {{/name}}
              {{#description}}
              <div class="summary lead">
                {{{markdown this}}}
              </div>
              {{/description}}
              <div class="row">
                {{#if highlights.length}}
                <div class="col-sm-6">
                  <div class="h4">Highlights</div>
                  <ul class="highlights">
                    {{#highlights}}
                    <li class="bullet">{{{markdown this}}}</li>
                    {{/highlights}}
                  </ul>
                </div>
                {{/if}}
                {{#if roles.length}}
                <div class="col-sm-6">
                  <div class="h4">Roles</div>
                  <ul class="roles">
                    {{#roles}}
                    <li class="bullet">{{{markdown this}}}</li>
                    {{/roles}}
                  </ul>
                </div>
                {{/if}}
              </div>
            </div>
            {{/each}}
            </div>
          </div>
        </section>
        {{/if}}

        {{#if resume.volunteer.length}}
        <section id="volunteer" class="row">
          <div class="col-sm-3">
            <h2>Volunteering</h2>
          </div>
          <div class="col-sm-9">
            <div class="row">
            {{#each resume.volunteer}}
            <div class="col-12 volunteer">
              <h3 class="strike-through">
                <span>{{organization}}</span>
                <span class="date">
                  {{startDate}} — {{#endDate}}{{.}}{{else}}Present{{/endDate}}
                </span>
              </h3>
              {{#website}}
              <div class="website">
                <a href="{{.}}">{{.}}</a>
              </div>
              {{/website}}
              {{#position}}
              <div class="position">
                {{.}}
              </div>
              {{/position}}
              {{#summary}}
              <div class="summary lead">
                {{{markdown this}}}
              </div>
              {{/summary}}
              {{#if highlights.length}}
              <div class="h4">Highlights</div>
              <ul class="highlights">
                {{#highlights}}
                <li class="bullet">{{{markdown this}}}</li>
                {{/highlights}}
              </ul>
              {{/if}}
            </div>
            {{/each}}
            </div>
          </div>
        </section>
        {{/if}}

        {{#if resume.education.length}}
        <section id="education" class="row">
          <div class="col-sm-3">
            <h2>Education</h2>
          </div>
          <div class="col-sm-9">
            <div class="row">
            {{#each resume.education}}
            <div class="col-12 education">
              <h3 class="strike-through">
                <span>{{institution}}</span>
                <span class="date">
                  {{startDate}} — {{#endDate}}{{.}}{{else}}Present{{/endDate}}
                </span>
              </h3>
              {{#area}}
              <div class="area">
                {{.}}
              </div>
              {{/area}}
              {{#studyType}}
              <div class="studyType">
                {{.}}
              </div>
              {{/studyType}}
              {{#if courses.length}}
              <h3>Courses</h3>
              <ul class="courses">
                {{#courses}}
                <li>{{.}}</li>
                {{/courses}}
              </ul>
              {{/if}}
            </div>
            {{/each}}
            </div>
          </div>
        </section>
        {{/if}}

        {{#if resume.awards.length}}
        <section id="awards" class="row">
          <div class="col-sm-3">
            <h2>Awards</h2>
          </div>
          <div class="col-sm-9">
            <div class="row">
            {{#each resume.awards}}
            <div class="col-12 award">
              <h3 class="strike-through">
                <span>{{title}}</span>
              </h3>
              {{#date}}
              <div class="date">
                <em>Awarded</em>
                {{.}}
              </div>
              {{/date}}
              {{#awarder}}
              <div class="awarder">
                <em>by</em>
                <strong>{{.}}</strong>
              </div>
              {{/awarder}}
              {{#summary}}
              <div class="summary lead">
                {{{markdown this}}}
              </div>
              {{/summary}}
            </div>
            {{/each}}
            </div>
          </div>
        </section>
        {{/if}}

        {{#if resume.publications.length}}
        <section id="publications" class="row">
          <div class="col-sm-3">
            <h2>Publications</h2>
          </div>
          <div class="col-sm-9">
            <div class="row">
            {{#each resume.publications}}
            <div class="col-12 publication">
              <h3 class="strike-through">
                <span>{{name}}</span>
                <span class="date">
                  {{releaseDate}}
                </span>
              </h3>
              {{#website}}
              <div class="website">
                <a href="{{.}}"></a>
              </div>
              {{/website}}
              {{#publisher}}
              <div class="publisher">
                <em>Published by</em>
                <strong>{{.}}</strong>
              </div>
              {{/publisher}}
              {{#summary}}
              <div class="summary lead">
                {{{markdown this}}}
              </div>
              {{/summary}}
            </div>
            {{/each}}
            </div>
          </div>
        </section>
        {{/if}}

        {{#if resume.skills.length}}
        <section id="skills" class="row">
          <div class="col-sm-3">
            <h2>Skills</h2>
          </div>
          <div class="col-sm-9">
            <div class="row">
            {{#each resume.skills}}
            <div class="col-sm-6 skill">
              {{#name}}
              <div class="name">
                <h3>{{.}}</h3>
              </div>
              {{/name}}
              {{#if keywords.length}}
              <ul class="keywords">
                {{#keywords}}
                <li>{{{markdown this}}}</li>
                {{/keywords}}
              </ul>
              {{/if}}
            </div>
            {{/each}}
            </div>
          </div>
        </section>
        {{/if}}

        {{#if resume.languages.length}}
        <section id="languages" class="row">
          <div class="col-sm-3">
            <h2>Languages</h2>
          </div>
          <div class="col-sm-9">
            <div class="row">
            {{#each resume.languages}}
            <div class="col-sm-6">
              {{#language}}
              <div class="language">
                <strong>{{.}}</strong>
              </div>
              {{/language}}
              {{#fluency}}
              <div class="fluency">
                {{.}}
              </div>
              {{/fluency}}
            </div>
            {{/each}}
            </div>
          </div>
        </section>
        {{/if}}

        {{#if resume.interests.length}}
        <section id="interests" class="row">
          <div class="col-sm-3">
            <h2>Interests</h2>
          </div>
          <div class="col-sm-9">
            <div class="row">
            {{#each resume.interests}}
            <div class="col-sm-6">
              {{#name}}
              <div class="name">
                <h3>{{.}}</h3>
              </div>
              {{/name}}
              {{#if keywords.length}}
              <ul class="keywords">
                {{#keywords}}
                <li>{{.}}</li>
                {{/keywords}}
              </ul>
              {{/if}}
            </div>
            {{/each}}
            </div>
          </div>
        </section>
        {{/if}}

        {{#if resume.references.length}}
        <section id="references" class="row">
          <div class="col-sm-3">
            <h2>References</h2>
          </div>
          <div class="col-sm-9">
            <div class="row">
            {{#each resume.references}}
            <div class="col-12">
              {{#if reference}}
              <blockquote class="blockquote">
                <div class="reference">
                {{{markdown reference}}}
                </div>
                {{#name}}
                <footer class="blockquote-footer">
                  {{.}}
                </footer>
                {{/name}}
              </blockquote>
              {{/if}}
            </div>
            {{/each}}
            </div>
          </div>
        </section>
        {{/if}}
      </div>
    </main>
  </body>
</html>
